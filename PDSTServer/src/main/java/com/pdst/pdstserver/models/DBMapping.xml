<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.pdst.pdstserver.models.Account">
        <table name="Account" schema="dbo" catalog="PDSTDB"/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="username">
                <column name="username"/>
            </basic>
            <basic name="password">
                <column name="password"/>
            </basic>
            <basic name="email">
                <column name="email"/>
            </basic>
            <basic name="phone">
                <column name="phone"/>
            </basic>
            <basic name="gender">
                <column name="gender"/>
            </basic>
            <basic name="imgUrl">
                <column name="img_url"/>
            </basic>
            <basic name="address">
                <column name="address"/>
            </basic>
            <basic name="numOfFollow">
                <column name="num_of_follow"/>
            </basic>
            <basic name="roleId">
                <column name="role_id"/>
            </basic>
            <basic name="status">
                <column name="status"/>
            </basic>
            <basic name="createdTime">
                <column name="created_time"/>
            </basic>
            <basic name="updatedTime">
                <column name="updated_time"/>
            </basic>
            <many-to-one name="roleByRoleId" optional="true" target-entity="com.pdst.pdstserver.models.Role">
                <join-column name="role_id" nullable="false" referenced-column-name="id"/>
            </many-to-one>

        </attributes>
    </entity>
